<p id="notice"><%= notice %></p>


    <% @home_posts.each do |home_post| %>
    <div class="panel panel-default">
      <div class="panel-heading"><h3>My Thoughts</h3></div>
      <div class="panel-body"><%= home_post.homepost %></div>
      </div>
      <button type="button" class="btn btn-default"><%= link_to 'New Post', new_home_post_path %></button>
      <button type="button" class="btn btn-default"> <%= link_to 'Edit Post', edit_home_post_path(home_post) %> </button>
      <button type="button" class="btn btn-default">  <%= link_to 'Delete Post', home_post, method: :delete, data: { confirm: 'Are you sure?' } %></button>
       <hr>

    <% end %>


<br>


